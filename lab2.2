		AREA RESET,DATA,READONLY
			DCD 0x20001000
			DCD Reset_Handler
		;ALIGN
	arr	DCD 2,5,6,7,8,9,1,3
		
		AREA MYCODE,CODE,READONLY
		ENTRY
		EXPORT Reset_Handler
		
	Reset_Handler
	count EQU 8
	
	start
		mov		R0,#0		;MAX=0
		ldr		r1,=arr
		mov		r2,#0
		ldr		r3,=count
		ldr		r5,[r1,r2]	;MIN=arr[0]
do
		ldr		r4,[r1,r2]
		cmp		r0,r4
		bgt		tiep1
		mov		r0,r4
tiep1
		cmp		r5,r4
		blt		tiep2
		mov		r5,r4
		
tiep2
		add		r2,r2,#4
		subs		r3,r3,#1
		cmp		r3,#0
		bgt		do
	stop b stop
		END
